{
  "name": "compass_menu",
  "fullName": "CompassMenu",
  "description": "A Mild Mouse Gesture",
  "author": "taku0 ( https://github.com/taku0 )",
  "license": "MPL 2.0",
  "id": "compass_menu@tatapa.org",
  "version": "2.0.3",
  "icon": "data/icon.png",
  "icon64": "data/icon64.png",
  "permissions": {
    "private-browsing": true
  },
  "preferences": [
    {
      "name": "openButton",
      "title": "Mouse button to open menu",
      "type": "menulist",
      "value": 2,
      "options": [
          {
              "value": "0",
              "label": "left_button"
          },
          {
              "value": "1",
              "label": "middle_button"
          },
          {
              "value": "2",
              "label": "right_button"
          },
          {
              "value": "3",
              "label": "fourth_button"
          },
          {
              "value": "4",
              "label": "fifth_button"
          }
      ]
    },
    {
      "name": "requireCtrl",
      "title": "Require Ctrl to open menu",
      "type": "bool",
      "value": false
    },
    {
      "name": "requireShift",
      "title": "Require Shift to open menu",
      "type": "bool",
      "value": false
    },
    {
      "name": "isCtrlSupress",
      "title": "Ctrl to supress menu",
      "type": "bool",
      "value": false
    },
    {
      "name": "isShiftSupress",
      "title": "Shift to supress menu",
      "type": "bool",
      "value": true
    },
    {
      "name": "labelDelay",
      "title": "Label delay in milliseconds",
      "type": "integer",
      "value": 500
    }
  ]
}
